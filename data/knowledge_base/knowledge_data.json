      "action_guide": {
        "phone": {
          "number": "1355",
          "hours": "평일 09:00-18:00",
          "script": "연금 수령 신청하려고 전화했습니다"
        },
        "online": {
          "url": "https://www.nps.or.kr",
          "app": "내 곁에 국민연금"
        },
        "visit": {
          "place": "가까운 국민연금공단 지사",
          "documents": [
